  "scripts": {
    "build": "parcel build src/index.html",
    "dev": "parcel serve src/index.html",
    "clean": "rm -rf dist",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{ts,tsx}' --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",

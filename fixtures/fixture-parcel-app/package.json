  "dependencies": {
    "react": "16.13.0",
    "react-dom": "16.13.0"
  },
  "devDependencies": {
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/webpack-env": "1.15.1",
    "parcel": "2.0.0-alpha.2.1",
    "typescript": "3.8.3"
  },
  "browserList": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ]
